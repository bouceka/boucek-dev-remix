{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --style=compressed",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "docker": " docker-compose -f docker-compose.yml up",
    "generate:css": "sass styles/:app/styles/",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:repo": "npm run lint -- --config .eslintrc.repo.js",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "postinstall": "prisma generate && remix setup node"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "@prisma/client": "^4.13.0",
    "@remix-run/node": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@remix-run/vercel": "^1.15.0",
    "@remix-validated-form/with-zod": "^2.0.5",
    "@vercel/node": "^2.6.2",
    "bcryptjs": "^2.4.3",
    "emailjs-com": "^3.2.0",
    "eslint-config-prettier": "^8.8.0",
    "gray-matter": "^4.0.3",
    "isbot": "^3.6.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-sass": "^8.0.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-markdown": "^8.0.7",
    "react-syntax-highlighter": "^15.5.0",
    "react-toastify": "^9.1.2",
    "remark-gfm": "^3.0.1",
    "remix-typedjson": "^0.1.7",
    "remix-validated-form": "^4.6.11",
    "tiny-invariant": "^1.3.1",
    "yup": "^1.1.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-syntax-highlighter": "^15.5.7",
    "eslint": "^8.39.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.13.0",
    "sass": "^1.62.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  }
}
